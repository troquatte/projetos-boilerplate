{
  "name": "projetos-boilerplate",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:dev": "npx ng build --configuration development",
    "build:prod": "npx ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:projetos-boilerplate": "node dist/projetos-boilerplate/server/server.mjs",
    "cy:open": "cypress open",
    "cy:ci": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^19.2.3",
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/fire": "^19.0.0",
    "@angular/forms": "^19.2.0",
    "@angular/material": "^19.2.3",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/platform-server": "^19.2.0",
    "@angular/router": "^19.2.0",
    "@angular/ssr": "^19.2.0",
    "express": "^4.18.2",
    "firebase-admin": "^13.2.0",
    "google-gax": "^4.4.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zod": "^3.24.2",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.0",
    "@angular-eslint/builder": "^19.2.1",
    "@angular-eslint/eslint-plugin": "^19.2.1",
    "@angular-eslint/eslint-plugin-template": "^19.2.1",
    "@angular-eslint/schematics": "^19.2.1",
    "@angular-eslint/template-parser": "^19.2.1",
    "@angular/cli": "^19.2.0",
    "@angular/compiler-cli": "^19.2.0",
    "@cypress/schematic": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^8.26.1",
    "@typescript-eslint/parser": "^8.26.1",
    "cypress": "^14.1.0",
    "cypress-angular-dev-server": "^1.0.2",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^3.6.1",
    "prettier": "^3.5.3",
    "start-server-and-test": "^2.0.11",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.26.1"
  },
  "lint-staged": {
    "*.ts": [
      "eslint src/ --fix",
      "prettier ./src/**/*.ts --write",
      "eslint --ignore-pattern '**/*.spec.ts' cypress/ --fix",
      "prettier ./cypress/**/*.ts --write",
      "git add ."
    ]
  }
}
